<% if current_user%>
<h1 class="headline_view">Add an Impact</h1>


<div class="impact_bubbles small cover_page">

    <%@impact_types.sort.each do |impact_type|%>

<a href=<%=impact_selector_id_path(impact_type.id)%>>
        <div class="impact_package">

            <div class="impact_bubble small">
              <img class="icon" src=<%=impact_type.image%>></img>

              <div class="impact_bubble_text">
                <h4><%=impact_type.name%></h4>
              </div>

            </div>
        </div>

</a>
    <%end%>
</div>
<%else%>
<div class="welcome">Welcome to the Impact Tracker</div>
<div class="welcome">Please sign in or sign up</div>
<%end%>
